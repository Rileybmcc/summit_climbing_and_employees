
<%= form_with url: "/gyms/#{@gym.id}", method: :patch, local: true do |form| %>

  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :opening_time %>
  <%= form.text_field :opening_time %>

  <%= form.label :closing_time %>
  <%= form.text_field :closing_time %>

  <%= form.label :currently_open %>
  <%= form.text_field :currently_open %>

  <%= form.label :customer_capacity %>
  <%= form.text_field :customer_capacity %>

  <%= form.submit 'Submit' %>

<% end %>
